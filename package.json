{
  "name": "window-switcher",
  "version": "0.0.1",
  "main": "main.js",
  "license": "MIT",
  "author": "Andrej Pancik",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ --out=out --icon=./icon.icns --overwrite"
  },
  "devDependencies": {
    "electron": "22.3.25",
    "electron-packager": "17.1.1"
  },
  "dependencies": {
    "iohook": "0.9.3",
    "node-window-manager": "2.2.4"
  },
  "iohook": {
    "targets": [
      "node-88",
      "electron-87"
    ],
    "platforms": [
      "darwin"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
